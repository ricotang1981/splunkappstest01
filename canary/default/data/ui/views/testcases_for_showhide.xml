<view template="dashboard.html" stylesheet="custom.css">
  <label>The ShowHide Module</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="appHeader" />
  <module name="SideviewUtils" layoutPanel="appHeader">
  </module>
  
  

  <module name="HTML" layoutPanel="viewHeader">
    <param name="html"><![CDATA[
      <h1>The ShowHide Module</h1>
    ]]></param>
  </module>
  


  <module name="HTML" layoutPanel="panel_row1_col1">
    <param name="html"><![CDATA[
      <div id="toBeShown" style="display:none;">should be shown onclick</div>

      <div id="toBeHidden">should be hidden onclick</div>
    ]]></param>
  </module>


  <module name="Button" layoutPanel="panel_row2_col1">
    <param name="label">Search</param>

    <module name="ShowHide">
      <param name="show">#toBeShown</param>
      <param name="hide">#toBeHidden</param>
    </module>


    <module name="Search">
      <param name="search"><![CDATA[
        index=_internal source="*metrics.log" group="per_sourcetype_thruput" | rename series as sourcetype | timechart max(eps) by sourcetype
      ]]></param>
      <param name="earliest">-4h</param>
      <param name="latest">now</param>
      <module name="ValueSetter">
        <param name="arg.charting.chart.stackMode">stacked</param>
        <module name="JSChart">
          
        </module>
      </module>
    </module>
  </module>

    

        
</view>

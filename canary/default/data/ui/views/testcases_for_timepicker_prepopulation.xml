      <module name="Search">
        <param name="search">search index=_internal | head 1000 | timechart count</param>

        <module name="ProgressIndicator" />
        <module name="Pager">
          <param name="count">5</param>
          
          <module name="Table" />
        </module>
        <module name="Button">
          <module name="Redirector">
            <param name="url">testcases_for_timepicker_prepopulation</param>
            <param name="arg.earliest">$search.timeRange.earliest$</param>
            <param name="arg.latest">$search.timeRange.latest$</param>
          </module>
        </module>
        <module name="HTML">
          <param name="html"><![CDATA[
           
            <p>
              $shared.timeRange.label$<br>
              <br>
              $shared.timeRange.earliest$<br>
              $shared.timeRange.latest$<br>
            </p>
           
           
          ]]></param>
        </module>
      </module>
    </module>



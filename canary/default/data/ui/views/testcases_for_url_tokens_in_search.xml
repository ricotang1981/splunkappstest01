<view template="dashboard.html">
  <label>testcases for URL tokens going right into searches.</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="appHeader" />
  <module name="SideviewUtils" layoutPanel="appHeader" />


  <module name="HTML" layoutPanel="viewHeader">
    <param name="html"><![CDATA[
    <h1>Testcases for CheckboxPulldown module </h1>
    ]]></param>
  </module>


  <module name="URLLoader" layoutPanel="appHeader" autoRun="True">
    <param name="keepURLUpdated">True</param>

    <module name="Search" layoutPanel="panel_row1_col1">
      <param name="search">index=_internal source="*metrics.log" group="per_sourcetype_thruput" | stats min(eps) as min avg(eps) as avg max(eps) as max by series $urlToken$</param>
      <param name="earliest">-1h</param>
      <param name="latest">now</param>
      
      <module name="Pager">
        
        <module name="Table"></module>
      </module>
        
      <module name="HTML">
        <param name="html"><![CDATA[
          <a href="?urlToken=%7C%20where%20min%3C1">click to load testcase url</a>
          <br>
          $urlToken$ is: <b>$urlToken$</b><br>
          the entire search is: <b>$search$</b>
        ]]></param>
      </module>

    </module>
  </module>


  
</view>
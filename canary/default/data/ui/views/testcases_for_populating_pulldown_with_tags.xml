<view isVisible="False" template="dashboard.html">
  <label>Drilldowns with tables and charts</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="appHeader" />
  <module name="SideviewUtils" layoutPanel="appHeader" />


  <module name="HTML" layoutPanel="viewHeader">
    <param name="html"><![CDATA[
    <h1>Inline drilldowns with tables and charts</h1>
    ]]></param>
  </module>

  
  <!-- EXAMPLE BEGIN -->
    <module name="Search" layoutPanel="panel_row1_col1" autoRun="True">
      <param name="search">| rest /servicesNS/admin/sideview_utils/data/tags | fields tag_name | sort tag_name</param>
      
      <module name="Pulldown">
        <param name="name">tag</param>
        <param name="label">Search on a tag:</param>
        <param name="valueField">tag_name</param>
        <param name="template">tag="$value$"</param>

        <module name="Search">
          <param name="search">$tag$ | head 1000 </param>
          
          <module name="Events"></module>
        </module>
      </module>
    </module>

</view>

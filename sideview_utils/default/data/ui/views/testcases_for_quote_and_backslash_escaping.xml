            click.searchTerms = <span class="shouldBe" s:shouldBe="path=&quot;C:\\foo\\bar&quot; quoted=&quot;foo \&quot;bar\&quot;&quot;">$click.searchTerms$</span><br>
          ]]></param>
        </module>

  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="appHeader" />
  <module name="SideviewUtils" layoutPanel="appHeader" />
  
  <module name="Messaging" layoutPanel="messaging" />

  <module name="HTML" layoutPanel="viewHeader">
    <param name="html"><![CDATA[
    <h1>Testcases for templatize and safeTemplatize methods</h1>
    ]]></param>
  </module>

  <module name="CustomBehavior" layoutPanel="panel_row1_col1" autoRun="True">
    <param name="customBehavior">templatizeTestCases</param>

    <module name="HTML">
      <param name="html"><![CDATA[
        <h2>Testing templatize replacement</h2>
        $output$
      ]]></param>
    </module>

  </module>


</view>

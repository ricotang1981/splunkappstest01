      <param name="arg.bar">BarValue</param>

      <module name="ValueSetter">
        <param name="arg.$foo$">only </param>
        <param name="arg.$bar$">zuul</param>

        <module name="HTML">
          <param name="customBehavior">highlightPassesAndFails</param>

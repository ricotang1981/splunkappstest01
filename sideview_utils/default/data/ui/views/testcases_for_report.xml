<view autoCancelInterval="90" isVisible="False" onunloadCancelJobs="true" template="dashboard.html" isSticky="False">
  <label>Testcases for Report module's permutations</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="appHeader" />
  <module name="SideviewUtils" layoutPanel="appHeader" />
  
  <module name="Messaging" layoutPanel="messaging" />

  <module name="HTML" layoutPanel="viewHeader">
    <param name="html"><![CDATA[
    <h1>Testcases for Report Module</h1>
    ]]></param>
  </module>

  <module name="NullModule" layoutPanel="panel_row1_col1" autoRun="True">

    <module name="Report">
      <param name="name">received</param>
      <param name="stat">$stat$</param>
      <param name="xField">$xField$</param>
      <param name="yField">$yField$</param>
      <param name="zField">$zField$</param>
      <param name="xFieldBins">$xFieldBins$</param>
      <param name="zFieldBins">$zFieldBins$</param>

      <module name="CustomBehavior">
        <param name="customBehavior">checkReportModuleTestCases</param>

        <module name="HTML">
          <param name="html"><![CDATA[
            <h2>test results</h2>
            <table class="splTable">
              <tr>
                <td>$successes$</td>
                <td>$failures$</td>
              </tr>
            </table>
          ]]></param>
        </module>
      </module>
    </module>

  </module>



</view>

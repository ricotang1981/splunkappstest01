          <param name="postProcess">search * $group$ | dedup $name$ | sort $name$</param>

          <module name="Search">
            <param name="search">index=_internal source="*metrics.log" $group$ $series$ | stats min(eps) avg(eps) max(eps) </param>
            <param name="earliest">-1h</param>
